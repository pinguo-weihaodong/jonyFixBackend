    "node": "6.10.1"
  },
  "main": "main.js",
  "scripts": {
    "electron-start": "electron .",
    "electron-build-dmg": "electron-packager .  佳尼跟拍 --overwrite --platform=darwin --arch=x64 --ignore=node_modules/electron-*",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=development webpack --progress",
    "release": "rimraf dist && cross-env NODE_ENV=production webpack --progress && npm run deploy",
    "deploy": "rimraf /Users/liu/git/live/public/cms/* && cp -rf dist/* /Users/liu/git/live/public/cms",
    "clean": "rimraf dist/",
    "cover": "nyc npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "dev": "webpack-dev-server",
    "start": "react-scripts-ts start",
    "postinstall": "rimraf ./node_modules/@types/node && npm run build",
    "lint": "npm run lint-ts && npm run lint-js && npm run stylefmt && npm run lint-css ",
    "lint-css": "stylelint \"./src/**/*.css\"",
    "lint-ts": "tslint \"src/**/*.tsx\" \"src/**/*.ts\"",
    "lint-js": "eslint \"**/*.js\" --fix --ignore-path .gitignore",
    "stylefmt": "stylefmt -R src/",
    "pretest": "npm run lint",
    "test": "tsc && ava --verbose",
    "test:watch": "concurrently \"tsc -w\"  \"ava --verbose --watch\"",
    "ghpage": "cross-env PUBLIC_PATH=gh npm run build && node ./scripts/gh-pages.js"
  },
